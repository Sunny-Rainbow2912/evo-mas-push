._tosAgreeButton
  background #0C3344
  box-sizing border-box
  height 60px
  margin 1px
  flex 1
  cursor pointer
  display flex
  justify-content center
  align-items center
  padding-top 3px
  box-sizing border-box
  transition var(--standard)
  font-family 'MainFont'
  transform translateY(0px) 
  text-transform uppercase
  flex-grow 1

._tosAgreeButton:hover
  background #002739
  transform translateY(-1px)
  box-shadow 0px 3px 4px var(--ghostY)

._tosContainer
    width: 100%;
    height: calc(100% - 30px)
    overflow-y: scroll